class Solution(object):
    def asteroidCollision(self, asteroids):
        n=len(asteroids)
        stack=[]
        for ind in range(0,n):
            if(asteroids[ind]>0):
                stack.append(asteroids[ind])
            else:
                while(len(stack)!=0 and stack[-1]>0 and stack[-1]<abs(asteroids[ind])):
                    stack.pop()
                if(len(stack)==0):
                    stack.append(asteroids[ind])
                elif(stack[-1]>0 and abs(asteroids[ind])==stack[-1] ):
                    stack.pop()
                elif(stack[-1]<0):
                    stack.append(asteroids[ind])
        return stack
