class Solution:
    def eraseOverlapIntervals(self, intervals: List[List[int]]) -> int:
        n=len(intervals)
        intervals.sort(key=lambda x:x[1])
        count=1
        freeTime=intervals[0][1]
        for row in range(1,n):
            if(intervals[row][0]>=freeTime):
                count+=1
                freeTime=intervals[row][1]
       return n-count
----o/p------
Input
intervals =
[[1,2],[2,3],[3,4],[1,3]]
Output
1
